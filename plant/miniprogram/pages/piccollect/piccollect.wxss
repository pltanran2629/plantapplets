/* pages/piccollect/piccollect.wxss */
.container {
  width: 100vw;
  height: 100vh;
  background-color: black;
}

.size {
  background-image: url(https://hotpotman.oss-cn-beijing.aliyuncs.com/basic/bgimage.png);
  background-size: 100% 100%;
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.sizetop {
  width: 100vw;
  height: 12vh;
  display: flex;
  flex-direction: row;
  align-items: flex-end;
}

.sizecon {
  height: 6vh;
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  width: 80%;
  position: relative;
}

.sizeconvir {
  background-color: #A5E5BF;
  height: 3.82vh;
  width: 23.5vw;
  padding-left: 10.5vw;
  font-size: 3.2vw;
  color: #2F6645;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  border-top-left-radius: 10rpx;
  /* border-right: 2.5vw solid rgba(97, 201, 139,0.78); */
  z-index: 4;
}

.sizeconreal {
  z-index: 4;
  width: 34.5vw;
  font-size: 3.2vw;
  color: #2F6645;
  height: 3.8vh;
  transform: skewX(40deg);
  background-color: #A5E5BF;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: absolute;
  left: 30.5vw;
  border-top-right-radius: 10rpx;
}

.sizeconreal>view {
  transform: skewX(-40deg);
}

.active {
  font-size: 3.4vw;
  color: white;
  background-color: rgba(97, 201, 139, 0.78);
  z-index: 3;
  border-top-right-radius: 10rpx;
}

.notactvir {
  box-shadow: -1vw -1vw 1vw rgba(0, 0, 0, 0.1);
}

.notactreal {
  box-shadow: 1vw -1vw 1vw rgba(0, 0, 0, 0.1);
}

.sizebot {
  /* margin-top: -0.2vh; */
  width: 90vw;
  height: 68vh;
  background-color: rgba(97, 201, 139, 0.78);
  border-radius: 4vw;
  z-index: 5;
}

.realgar {
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.realgargenscoll {
  margin-top: 3vh;
  height: 64vh;
  width: 95%;
}

.realtime {
  color: white;
  font-size: 2.9vw;
  letter-spacing: 0.01rem;
  display: flex;
  margin-bottom: 2vh;
  flex-direction: row;
  justify-content: flex-end;
  align-items: baseline;
  width: 93%;
}

.realtime>image {
  width: 3vw;
  height: 2vw;
  margin-left: 1vw;
}

.realcol {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.realcolone {
  width: 50%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  position: relative;
  margin-bottom: 2.5vh;
}

.colonebg {
  width: 35vw;
  height: 21vh;
}

.virfolimage {
  position: absolute;
  top: 2.5vh;
  z-index: 2;
  width: 28vw;
  height: 13vh;
  /* transform: rotateX(180deg); */
}

.coldes {
  position: absolute;
  top: 16.2vh;
  text-align: center;
  color: #1A5629;
}

.namecn {
  font-size: 2.5vw;
  letter-spacing: 0.3rem;
}

.nameen {
  font-size: 1.8vw;
  max-width: 22vw;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.realfolimage {
  position: absolute;
  top: 3vh;
  z-index: 2;
  width: 28vw;
  height: 13vh;
}

.model {
  background-color: #2d2d2d;
  width: 100vw;
  height: 100vh;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99999;
}

.modelbg {
  background-image: url(https://hotpotman.oss-cn-beijing.aliyuncs.com/basic/bgimagezhao.png);
  background-size: 100% 100%;
  z-index: 1000000;
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.virgreenbg {
  width: 85.8vw;
  height: 53vh;
  position: absolute;
  top: 50%;
  left: 50%;
  transform-origin: center;
  transform: translate(-50%, -50%);
  animation: scalimagevirgreenbg 0.6s;
}

.virgreengong {
  width: 59vw;
  height: 40vh;
  position: absolute;
  top: 29vh;
  animation: scalimagevirgreengong 0.6s;
}

.virgreenfol {
  width: 60vw;
  height: 25vh;
  position: absolute;
  top: 44vh;
  animation: scalimagevirgreenflo 0.6s;
}

