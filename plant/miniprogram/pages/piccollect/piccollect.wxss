  0% {
    height: 0vh;
  }

  20% {
    height: 7vh;
  }

  40% {
    height: 14vh;
  }

  60% {
    height: 21vh;
  }

  80% {
    height: 27vh;
  }

  100% {
    height: 32vh;
  }
}

.realgreenfol {
  width: 60vw;
  height: 25vh;
  position: absolute;
  top: 30.5vh;
  animation: scalimagerealflo 0.6s;
  /* transform: rotate(180deg); */
}

@keyframes scalimagerealflo {
  0% {
    height: 0vh;
  }

  20% {
    height: 4vh;
  }

  40% {
    height: 8vh;
  }

  60% {
    height: 12vh;
  }

  80% {
    height: 16vh;
