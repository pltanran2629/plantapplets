@keyframes scalimagevirgreengong {
  0% {
    height: 0vh;
  }

  20% {
    height: 8vh;
  }

  40% {
    height: 16vh;
  }

  60% {
    height: 24vh;
  }

  80% {
    height: 32vh;
  }

  100% {
    height: 38vh;
  }
}

@keyframes scalimagevirgreenbg {
  0% {
    height: 0vh;
  }

  20% {
    height: 10vh;
  }

  40% {
    height: 20vh;
  }

  60% {
    height: 30vh;
  }

  80% {
    height: 40vh;
  }

  100% {
    height: 53vh;
  }
}

@keyframes scalimagevirgreenflo {
  0% {
    height: 0vh;
  }

  20% {
    height: 5vh;
  }

  40% {
    height: 10vh;
  }

  60% {
    height: 15vh;
  }

  80% {
    height: 20vh;
  }

  100% {
    height: 25vh;
  }
}

.bottom {
  position: absolute;
  bottom: 10vh;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  width: 100vw;
}

.savaimage {
  width: 30vw;
  height: 4.5vh;
  background-color: #606060;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: white;
  letter-spacing: 00.05rem;
  font-size: 2.8vw;
}

.plantname {
  width: 30vw;
  height: 4.3vh;
  background-image: linear-gradient(#31AD77, #dafdde);
  border-radius: 2.5vh;
  position: absolute;
  top: 70vh;
  color: white;
  text-align: center;
  line-height: 2em;
  animation: showname 0.6s;
}

@keyframes showname {
  0% {
    height: 0vh
  }

  50% {
    height: 2vh
  }

  100% {
    height: 4.3vh
  }

}

#cavansId {
  width: 100vw;
  height: 100vh;
}

.realgreenbg {
  width: 85.8vw;
  height: 53vh;
  position: absolute;
  top: 50%;
  left: 50%;
  transform-origin: center;
  transform: translate(-50%, -50%);
  animation: scalimagevirgreenbg 0.6s;
