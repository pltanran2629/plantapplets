    margin-top: 0.8vh;
  }
  .cenbot{
    width: 100%;
    height: 55vh;
    padding-top: 2.5vh;
    display: flex;
    flex-direction: column;
    /* justify-content: center; */
    /* align-items: center; */
    position: relative;
    overflow: hidden;
  }
  .cenbotbg{
    z-index: 2;
    background-color:#4ABC78;
    left: 50%;
    transform:translateX(-50%) ;
    position: absolute;
    top: 6vh;
    width: 81vw; 
    height: 49vh;
    border-radius: 34rpx;
    box-shadow: 0 0 40rpx rgba(0,0,0, 0.2);
    display: flex;
    flex-direction: column;
    justify-content:center;
    align-items: flex-end;
    overflow: hidden;
  }
  .cenbotbg1{
    top: 0vh;
    left: 50%;
    /* animation: bubble-in 1s both; */
    transform-origin: center;
    transform: translateX(-50%) scale(0.85);
    background-color:#34A562;
    z-index: 1;

    position: absolute;
    overflow: hidden;
    width: 81vw; 
    height: 49vh;
    border-radius: 34rpx;
    box-shadow: 0 0 40rpx rgba(0,0,0, 0.2);
    display: flex;
    flex-direction: column;
    justify-content:center;
    align-items: flex-end;
  }
  @keyframes op{
    0%{
      opacity: 0;
    }
    10%{
      opacity: 0.1;
    }
    20%{
      opacity: 0.2;
    }
    30%{
      opacity: 0.3;
    }
    40%{
      opacity: 0.4;
    }
    50%{
      opacity: 0.5;
    }
    60%{
      opacity: 0.6;
    }
    70%{
      opacity: 0.7;
    }
    80%{
      opacity: 0.8;
    }
    90%{
      opacity: 0.9;
    }
    100%{
      opacity:1;
    }
  }
  .anim{
     animation: op 200ms both;
  }
  .cenbotbg2{
    top: -6vh;
    left: 50%;
    transform-origin: center;
    transform: translateX(-50%) scale(0.7);
    background-color:#227A46;
    z-index: 0;
    position: absolute;
    overflow: hidden;
    width: 81vw; 
    height: 49vh;
    border-radius: 34rpx;
    box-shadow: 0 0 40rpx rgba(0,0,0, 0.2);
    display: flex;
    flex-direction: column;
    justify-content:center;
    align-items: flex-end;
  }
  .cenbotitem{
    background-color: white;
