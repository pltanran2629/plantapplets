  }
  .cenbotplantdetailtext{
    font-size: 1.3vh;
  }
  .cenbottime{
    font-size: 1.2vh;
    padding:0.2vh 1.8vw ;
  }
  .cenbottimetext{
    font-size: 1.7vh;
    color: #0C5B1B;
  }
  .plantimg{
    position: absolute;
    width: 40vw;
    max-height: 46.5vh;
    left: 1vw;
    top: 50%;
    transform: translateY(-50%);
    /* opacity: 0.8; */
  }
  .item2{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }
  .conbotcenone>image{
    width: 7.8vw;
    height: 4vh;
  }
  .conbotcenone{
    display: flex;
    flex-direction: row;
    justify-content:space-around;
    width: 90%;
  }
  .conbotcentwo,.conbotcenthree{
    display: flex;
    flex-direction: row;
    justify-content:space-around;
    width: 90%;
    text-align: center;
    color: #1A7C31;
  }
  .conbotcentwo>view{
    width: 33.3%;
    padding-top: 1vh;
    font-size: 0.9vw;
  }
.conbotcenthree>view{
  font-size: 1.4vh;
  padding-top: 0.4vh;
  width: 33%;
}
