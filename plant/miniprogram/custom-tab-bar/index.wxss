.tabbararea{
  width: 100%;
  /* height: 210rpx; */
  height: 13.12vh;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  flex-direction: row;
  justify-content: center;
  font-size: 18rpx;
  z-index: -5;
}
.tabarea{
  width: 90%;
  /* height: 165rpx; */
  height: 10.3vh;
  margin-top: 11rpx;
  box-shadow:rgba(0, 0, 0, 0.4) 0px 0px 10px;
  border-radius: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  background-color: white;
}
.tabbar1{
  padding-top: 0.8vh;
  width: 12%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.tabimg1{
  width: 3vh;
  height:3.2vh;
}
.tabtext{
  padding-top: 0.5vh;
}


